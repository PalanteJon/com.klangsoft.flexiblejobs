<?xml version="1.0"?>
<extension key="com.klangsoft.flexiblejobs" type="module">
  <file>flexiblejobs</file>
  <name>Flexible Jobs</name>
  <description>Improves the flexibility of scheduled jobs by adding "Yearly", "Quarterly", "Monthly", and "Weekly" run frequencies. It also allows one to specify a specific date/time that a scheduled job should first/next run.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>David Reedy Jr</author>
    <email>dreedy@klangsoft.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/konadave/com.klangsoft.flexiblejobs</url>
    <url desc="Documentation">https://github.com/konadave/com.klangsoft.flexiblejobs/blob/master/README.md</url>
    <url desc="Support">https://github.com/konadave/com.klangsoft.flexiblejobs/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-12-24</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.6</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Flexiblejobs</namespace>
  </civix>
</extension>
